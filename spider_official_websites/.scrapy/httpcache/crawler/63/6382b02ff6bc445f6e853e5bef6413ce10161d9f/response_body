<script type="text/javascript">

	 $('.flexslider').flexslider({
	   animation: "slide",
	   controlNav: "thumbnails",
	   thumbCaptions: true,
	   directionNav: false,
	   slideshowSpeed: 10000,
	  });
</script>


<script type="text/javascript">

	$.each($(".paginator"), function() {
		count = Math.ceil($(this).find(".campaign_view_responsive").length/3.0);
		$(this).find(".next_button").attr("data-last", count);
		$(this).find(".page_counter span:last").html(count);
		$(this).attr("data-page", 0);
	});

	$(".next_button").bind("click", function() {
		var $paginator = $(this).closest(".paginator");
		var page = parseInt($paginator.attr("data-page"), 10);

		if (page + 1 < parseInt($(this).attr("data-last"), 10)) {
			page += 1;
			$paginator.attr("data-page", page);
			$paginator.find(".slider > div").animate({left: page * -987});
			$paginator.find(".page_counter span:first").html(page + 1);
		}
	});
	$(".prev_button").bind("click", function() {
		var $paginator = $(this).closest(".paginator");
		var page = parseInt($paginator.attr("data-page"), 10);

		if (page - 1 >= 0) {
			page -= 1;
			$paginator.attr("data-page", page);
			$paginator.find(".slider > div").animate({left: page * -987});
			$paginator.find(".page_counter span:first").html(page + 1);
		}
	});

	var category_id;
	var category_label;
	$(".category_select").change(function() {
		category_id = $(this).val();
		category_label = $(".category_select option:selected").text();

		$(".loader").show();
		$(".category_campaigns .campaign_view_responsive").fadeOut(1000);

		$.ajax({
			url: "/all?category=" + $(this).val() + "&l=0",
			data: {},
			type: "GET",
			success: function(data) {
				$(".loader").hide();
				$(".category_campaigns .paginator").attr("data-page", 0);
				$(".category_campaigns .paginator .slider > div").css({left: 0});
				$(".category_campaigns .paginator .page_counter span:first").html(1);

				$(".category_campaigns .slider > div").html("").hide().append($(data)).fadeIn(1000);
				$(".category_campaigns").animate({backgroundColor: getColorCode(category_id)});

				$.each($(".category_campaigns .campaign_view_responsive canvas"), function(i, val) {
					Thunderclap.draw.flag(val);
				});

				count = Math.ceil($(".category_campaigns").find(".campaign_view_responsive").length/3.0);
				$(".category_campaigns").find(".next_button").attr("data-last", count);
				$(".category_campaigns").find(".page_counter span:last").html(count);
				$(".category_campaigns").attr("data-page", 0);

				ga('send', 'pageview', '/category/' + category_label);
			}
		});
	});

	function getColorCode(category) {
		switch(category) {
			case "9": return '#e3d61d';
			case "31": return '#D6CBBE';
			case "10": return '#faaa43';
			case "3": return '#2b4457';
			case "27": return '#0F8CD2';
			case "15": return '#FF9912';
			case "13": return '#008B00';
			case "28": return '#000066';
			case "1": return '#e085c4';
			case "14": return '#E0427F';
			case "5": return '#e44500';
			case "4": return '#7dc411';
			case "6": return '#000000';
			case "8": return '#31b57d';
		}
	}
// });

</script>



	</body>
</html>

